{
  "name": "ngxe-source",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ngxe:start": "ng run ngxe:serve --proxy-config proxy.conf.json",
    "ngxe:build": "ng run ngxe:build:production",
    "ngxe:build:demo": "ng run ngxe:build:demo",
    "test": "ng run ngxe:test",
    "demo:start": "ng run demo:serve --port=4201",
    "demo:start:uk": "ng run demo:serve:uk",
    "demo:xi": "ng run demo:extract-i18n",
    "backend:start": "node dist/backend/backend/app -p=projects/demo/ngxe.json",
    "backend:build": "tsc --project ./projects/backend/tsconfig.json",
    "backend:build:watch": "rimraf ./dist/backend && tsc --project ./projects/backend/tsconfig.json --watch",
    "release:build": "npm run ngxe:build && npm run backend:build",
    "release": "npm run release:build && node release.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
